	url = {http://citeseerx.ist.psu.edu/viewdoc/download?doi=10.1.1.5.5375&rep=rep1&type=pdf},
	abstract = {In this paper we study the over-constrained Airport Gate Assignment Problem where the objectives are to minimize the number of ungated ﬂights and the total walking distances or connection times. We approach the problem by ﬁrst using a greedy algorithm to minimize ungated ﬂights and by developing exchange moves which then facilitate the use of heuristics. Simulated Annealing and a hybrid of Simulated Annealing and Tabu Search are used. Experimental results are good and exceed those previously obtained.},
	language = {en},
	author = {Ding, H and Lim, A and Rodrigues, B and Zhu, Y},
